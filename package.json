{
  "name": "velum-monorepo",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@7.12.2",
  "description": "Monorepo of velum",
  "license": "MIT",
  "contributors": [
    "sxzz",
    "ShenQingchuan"
  ],
  "scripts": {
    "build": "turbo run build --filter=\"./packages/*\"",
    "dev": "turbo run dev --filter=\"./packages/*\"",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.json,.md",
    "lint:fix": "pnpm run lint --fix",
    "test": "vitest",
    "publish": "pnpm build && changeset publish",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.7",
    "@changesets/cli": "^2.25.0",
    "@sxzz/eslint-config-prettier": "^2.4.4",
    "@sxzz/eslint-config-ts": "^2.4.4",
    "@types/node": "*",
    "@velumjs/yorkie": "^1.0.0",
    "bumpp": "^8.2.1",
    "eslint": "8.22.0",
    "eslint-define-config": "^1.7.0",
    "fast-glob": "^3.2.12",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "tsup": "^6.2.3",
    "tsx": "^3.9.0",
    "turbo": "^1.5.5",
    "typescript": "^4.8.4",
    "vite": "^3.1.8",
    "vitepress": "1.0.0-alpha.20",
    "vitest": "^0.23.4"
  },
  "engines": {
    "node": ">=14.19.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,json}": [
      "eslint"
    ]
  }
}
